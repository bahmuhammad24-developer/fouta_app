# This line loads Flutter's plugin helper scripts. It must be at the top.
require File.expand_path(File.join('..', '.flutter-plugins-dependencies'), __FILE__)

target 'Runner' do
  use_frameworks!
  use_modular_headers!

  flutter_install_plugin_pods('Runner')
end

post_install do |installer|
  flutter_post_install(installer)
  
  # As per our project history, a custom script was added here to remove a
  # problematic compiler flag. That logic should be preserved within this block.
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      # This line is an example of the custom fix mentioned in the project history.
      config.build_settings.delete('GCC_WARN_UNUSED_VARIABLE')
    end
  end
end